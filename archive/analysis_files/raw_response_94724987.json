            }
          },
          {
            "id": "3f683dcb-48f9-4f7d-a4bc-526d0886c0b5",
            "text": "The box arrived damanged",
            "author": {
              "nickname": "Box arrived damged",
              "external_id": "10106881707"
            },
            "title": "Box dented",
            "rating": {
              "value": 2.0,
